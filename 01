#include <stdio.h>
#include <stdlib.h>

int main(){
    int a,b;

    printf("put two numbers:\n");
    scanf("%i %i", &a, &b);

    int addition = a +b;
    int subtraction = a-b;
    int multiplication = a *b;
    int division = a/b;
    int modulus = a%b;


    printf("result: %i + %i = %i\n", a,b, addition);
    printf("result: %i - %i = %i\n", a, b,subtraction);
    printf("result: %i * %i = %i\n", a, b, multiplication);
    printf("result: %i / %i = %i\n", a, b, division);
    printf("result: %i %% %i = %i\n", a, b, modulus);

     return 0;
}
