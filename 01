#include <stdio.h>
#include <stdlib.h>

int main(){
    int a;

    printf("input the year:\n ");
    scanf("%d", &a);

    int result = 0;

    if ((a % 4 == 0 && a % 100 != 0) || (a % 400 == 0)) {
         result = 1;
        }

        printf( "is the a %d the leap year? %i\n", a,result);

        return 0;
    }

